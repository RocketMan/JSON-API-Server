{
  "name": "enm/json-api-server",
  "description": "Abstract server-side php implementation of the json api specification (jsonapi.org)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "marien@eosnewmedia.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Enm\\JsonApi\\Server\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enm\\JsonApi\\Server\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "enm/json-api-common": "dev-develop@dev",
    "psr/http-message": "^1.0",
    "guzzlehttp/psr7": "^1.4",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "provide": {
    "enm/json-api-server-bundle": "^1.0"
  }
}
